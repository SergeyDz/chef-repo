<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>

<!-- generated by Chef for <%= node['fqdn'] %>, changes will be overwritten -->

<configuration>
  <% (@options || {}).sort.each do |name, value| %>
  <property>
    <name><%= name %></name>
    <value><%= value %></value>
  </property>
  <% end %>
</configuration>
